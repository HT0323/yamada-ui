{
  "name": "yamada-ui",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.15.0",
  "license": "MIT",
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui"
  },
  "scripts": {
    "prepare": "lefthook install",
    "storybook": "pnpm storybook:react",
    "storybook:react": "pnpm --prefix packages/react storybook",
    "storybook:serve": "pnpm --prefix packages/react storybook:serve",
    "storybook:build": "pnpm --prefix packages/react storybook:build",
    "prebuild": "pnpm build:cli && pnpm gen:tokens",
    "build": "turbo run build --filter=./packages/**",
    "build:cli": "pnpm --prefix packages/cli build",
    "build:react": "pnpm --prefix packages/react build",
    "build:utils": "pnpm --prefix packages/utils build",
    "lint": "turbo run lint --filter=./packages/**",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "lint:react": "pnpm --prefix packages/react lint",
    "lint:utils": "pnpm --prefix packages/utils lint",
    "format": "turbo run format --filter=./packages/**",
    "format:react": "pnpm --prefix packages/react format",
    "format:utils": "pnpm --prefix packages/utils format",
    "typecheck": "turbo run typecheck --filter=./packages/**",
    "typecheck:react": "pnpm --prefix packages/react typecheck",
    "typecheck:utils": "pnpm --prefix packages/utils typecheck",
    "test": "turbo run test --filter=./packages/**",
    "test:react": "pnpm --prefix packages/react test",
    "test:utils": "pnpm --prefix packages/utils test",
    "test:issue": "tsx scripts/coverage-issues",
    "spellcheck": "cspell \"**/*.{js,jsx,mjs,ts,tsx,mts,md,mdx}\" --quiet",
    "quality": "pnpm format && pnpm lint && pnpm typecheck && pnpm test",
    "gen:css": "tsx scripts/generate-css && eslint packages/core/src/styles.ts --fix --flag unstable_ts_config",
    "gen:tokens": "yamada-cli tokens packages/theme/src/index.ts --out packages/core/src/generated-theme.types.ts && eslint packages/core/src/generated-theme.types.ts --fix --flag unstable_ts_config",
    "gen:icons": "tsx scripts/generate-icons && eslint packages/react/src/components/icon/icons/index.ts packages/react/src/components/icon/icons/index.types.ts --fix --flag unstable_ts_config",
    "gen:docs": "pnpm --filter=./packages/components/** gen:docs",
    "changelog": "tsx scripts/changelog",
    "changelog:ci": "tsx scripts/changelog --current",
    "changelog:latest": "tsx scripts/changelog --latest",
    "version:dev": "changeset version --snapshot dev",
    "version:next": "changeset version --snapshot next",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev",
    "release:next": "changeset publish --tag next",
    "report:insights": "tsx scripts/report-insights",
    "notification:release": "tsx scripts/notification-release",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log node_modules",
    "clean:all": "pnpm clean && pnpm rimraf .turbo *.log .eslintcache coverage storybook-static trace node_modules",
    "docs:dev": "pnpm --prefix docs dev",
    "docs:start": "pnpm --prefix docs start",
    "docs:build": "pnpm --prefix docs build",
    "docs:lint": "pnpm --prefix docs lint",
    "docs:format": "pnpm --prefix docs format",
    "docs:format:check": "pnpm --prefix docs format:check",
    "docs:typecheck": "pnpm --prefix docs typecheck",
    "docs:test": "pnpm --prefix docs test",
    "docs:content": "pnpm --prefix docs gen:content",
    "docs:changelog": "pnpm --prefix docs gen:changelog",
    "docs:theme": "pnpm --prefix docs gen:theme",
    "docs:styled-system": "pnpm --prefix docs gen:styled-system",
    "docs:props": "pnpm --prefix docs gen:props",
    "docs:search": "pnpm --prefix docs gen:search",
    "docs:icon-tags": "pnpm --prefix docs gen:icon-tags",
    "docs:contributors": "pnpm --prefix docs gen:contributors",
    "docs:translate": "pnpm --prefix docs gen:translate",
    "docs:tokens": "pnpm --prefix docs gen:tokens",
    "docs:label": "pnpm --prefix docs gen:label",
    "next-pages:dev": "pnpm --prefix examples/next/pages dev",
    "next-app:dev": "pnpm --prefix examples/next/app dev",
    "vite:dev": "pnpm --prefix examples/vite dev",
    "remix:dev": "pnpm --prefix examples/remix dev",
    "hono:dev": "pnpm --prefix examples/hono dev"
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11",
    "@changesets/get-release-plan": "^4.0.6",
    "@changesets/types": "^6.0.0",
    "@clack/prompts": "^0.8.2",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@cspell/eslint-plugin": "^8.17.1",
    "@eslint/compat": "^1.2.4",
    "@faker-js/faker": "^9.3.0",
    "@koralle/vitest-axe": "^0.1.1",
    "@lucide/lab": "^0.1.2",
    "@octokit/request-error": "^6.1.5",
    "@octokit/rest": "^21.0.2",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-backgrounds": "^8.4.7",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-measure": "^8.4.7",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/addon-viewport": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/cli": "^8.4.7",
    "@storybook/preview-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test-runner": "^0.20.1",
    "@storybook/theming": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/babel__traverse": "^7.20.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/parser": "^8.18.1",
    "@typescript/analyze-trace": "^0.10.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/eslint-plugin": "^1.1.19",
    "@vitest/ui": "^2.1.8",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "@yamada-ui/test": "workspace:*",
    "@yamada-ui/utils": "workspace:*",
    "axe-playwright": "^2.0.3",
    "axios": "^1.7.9",
    "chalk": "^5.4.0",
    "commander": "^12.1.0",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "execa": "9.3.1",
    "find-packages": "^10.0.4",
    "glob": "^11.0.0",
    "globals": "^15.14.0",
    "jiti": "^2.4.2",
    "jsdom": "^25.0.1",
    "lefthook": "^1.9.3",
    "list-it": "^1.3.13",
    "node-fetch": "^3.3.2",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.1",
    "react-icons": "^5.4.0",
    "rimraf": "^6.0.1",
    "storybook": "^8.4.7",
    "storybook-dark-mode": "^4.0.2",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
